{
    "language": "PYTHON",
    "main-file": "run_mxnet_mnist",
    "experiment-name": "mxnet-mnist-simple",
    "polling-time": 5,
    "resources": {
	"my-machine": {
	    "scheduler": "local",
	    "max-concurrent": 1,
	    "max-finished-jobs": 100
	}
    },
    "variables": {
	"num_hidden_layers": {
	    "type": "INT",
	    "size": 1,
	    "min": 1,
	    "max": 8
	},
	"num_neurons": {
	    "type": "INT",
	    "size": 1,
	    "min": 1,
	    "max": 100
	},
	"lr": {
	    "type": "FLOAT",
	    "size": 1,
	    "min": 0.001,
	    "max": 0.1
	},
	"lr_factor_idx": {
	    "type": "INT",
	    "size": 1,
	    "min": 0,
	    "max": 4
	},
	"lr_step_epoch_idx": {
	    "type": "INT",
	    "size": 1,
	    "min": 0,
	    "max": 2
	}
    }
}
