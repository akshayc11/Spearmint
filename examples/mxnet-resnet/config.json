{
    "language": "PYTHON",
    "main-file": "run_mxnet_resnet",
    "experiment-name": "mxnet-resnet-simple",
    "polling-time": 5,
    "resources": {
	"my-machine": {
	    "scheduler": "local",
	    "max-concurrent": 1,
	    "max-finished-jobs": 100
	}
    },
    "variables": {
	"per_unit": {
	    "type": "INT",
	    "size": 1,
	    "min": 1,
	    "max": 5
	},
	"num_segments": {
	    "type": "INT",
	    "size": 1,
	    "min": 1,
	    "max": 4
	},
	"lr": {
	    "type": "FLOAT",
	    "size": 1,
	    "min": 0.01,
	    "max": 0.1
	},
	"lr_factor_idx": {
	    "type": "INT",
	    "size": 1,
	    "min": 0,
	    "max": 4
	},
	"lr_step_epoch_idx": {
	    "type": "INT",
	    "size": 1,
	    "min": 0,
	    "max": 5
	}
    }
}
